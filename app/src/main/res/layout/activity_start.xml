<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".StartActivity">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="333dp"
        android:layout_height="312dp"
        android:layout_marginTop="98dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/burgerr" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="29dp"
        android:textAlignment="center"
        android:textStyle="bold"
        android:fontFamily="@font/yeonsung"
        android:text="Enjoy Restaurant Quality Meals\nat Home"
        android:textSize="20sp"
        android:textColor="@color/greenStart"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/go_user_login_btn"
        android:layout_width="157dp"
        android:layout_height="57dp"
        android:layout_marginBottom="20dp"
        android:background="@drawable/green_btn"
        android:fontFamily="@font/yeonsung"
        android:text="Next"
        android:textStyle="bold"
        android:outlineSpotShadowColor="@color/greenStart"
        android:outlineAmbientShadowColor="@color/greenStart"
        android:elevation="3dp"
        android:gravity="center"
        android:textColor="@color/white"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="78dp"
        android:layout_height="32dp"
        android:layout_marginBottom="32dp"
        android:fontFamily="@font/yeonsung"
        android:text="Design By\nNeatRoots"
        android:textStyle="bold"
        android:textAlignment="center"
        android:textColor="@color/greenStart"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/button" />
</androidx.constraintlayout.widget.ConstraintLayout>